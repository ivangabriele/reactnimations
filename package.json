{
  "name": "reactnimations",
  "description": "A library of configurable React-componentized canvas animations.",
  "license": "MIT",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "prepare": "husky install",
    "start": "next start",
    "test": "npm run test:lint",
    "test:lint": "eslint .",
    "preversion": "npm test",
    "postversion": "node ./scripts/finalizeVersionBump.js",
    "packages:build": "npm run build --workspaces"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "next": "12.1.0",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-jsx": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-react": "7.14.5",
    "@ivangabriele/eslint-config-react": "1.5.0",
    "@ivangabriele/prettier-config": "3.0.2",
    "@rollup/plugin-babel": "5.3.0",
    "@types/react-select": "5.0.0",
    "husky": "7.0.2",
    "lint-staged": "12.1.1",
    "prettier": "2.3.2",
    "react-select": "5.0.0",
    "rollup": "2.56.3",
    "rollup-plugin-cleaner": "1.0.0",
    "rollup-plugin-sizes": "1.0.4",
    "rollup-plugin-sourcemaps": "0.6.3",
    "shelljs": "0.8.5"
  },
  "workspaces": [
    "./packages/*"
  ],
  "prettier": "@ivangabriele/prettier-config",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
